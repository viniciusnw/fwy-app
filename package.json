{
  "name": "@fasting/app",
  "version": "1.0.1",
  "homepage": "https://www.fastingwithyara.com/",
  "private": true,
  "scripts": {
    "====": "===DEV",
    "i": "yarn && cd ios && pod install && cd .. && yarn patch",
    "clear": "rm -rf node_modules && rm -rf package-lock.json && rm -rf yarn.lock && rm -rf ios/Pods && rm -rf ios/Podfile.lock && cd android && ./gradlew clean",
    "pause": "read -p 'Press Enter to continue'",
    "patch": "npx patch-package",
    "==========": "==GRAPHQL",
    "graphql:generate-ts-schema": "apollo codegen:generate src/Config/graphql/graphql-schema.ts --target=typescript --outputFlat",
    "===": "===BUNDLER",
    "start:bundler": "react-native start --reset-cache",
    "android:bundler": "react-native bundle --platform android --dev false --entry-file index.android.js --bundle-output android/app/src/main/assets/index.android.bundle",
    "ios:bundler": "react-native bundle --platform ios --dev false --entry-file index.ios.js --bundle-output ios/main.jsbundle",
    "=====": "===STORY",
    "android:story": "react-native run-android --variant=storybookDebug",
    "ios:story": "react-native run-ios --scheme='Storybook' --configuration='Debug' --simulator='iPhone 11'",
    "======": "===IOS",
    "ios:fas:dev": "react-native run-ios --scheme='Fasting' --configuration='Debug' --simulator='iPhone 11'",
    "ios:fas:prod": "react-native run-ios --scheme='Fasting' --configuration='Release' --simulator='iPhone 11'",
    "ios:adm:dev": "react-native run-ios --scheme='FastingAdm' --configuration='Debug' --simulator='iPhone 11'",
    "ios:adm:prod": "react-native run-ios --scheme='FastingAdm' --configuration='Release' --simulator='iPhone 11'",
    "=======": "===ANDROID",
    "android:fas:dev": "react-native run-android --variant=fastingDebug",
    "android:fas:prod": "react-native run-android --variant=fastingRelease",
    "android:adm:dev": "react-native run-android --variant=fastingAdmDebug",
    "android:adm:prod": "react-native run-android --variant=fastingAdmRelease",
    "========": "===BUILD/Android",
    "android:release:keygen": "keytool -genkey -v -keystore my-release-key.keystore -alias fasting -keyalg RSA -keysize 2048 -validity 10000",
    "=========": "===BUILD/ios",
    "ios:fas:up:bundler": "node -r ts-node/register scripts/ios.up-build.ts fas",
    "ios:fas:up:version:patch": "node -r ts-node/register scripts/ios.up-version.ts fas patch",
    "ios:fas:up:version:minor": "node -r ts-node/register scripts/ios.up-version.ts fas minor",
    "ios:fas:up:version:major": "node -r ts-node/register scripts/ios.up-version.ts fas major",
    "ios:fas:clean:build": "rm -rf ios/build/Fasting-Release.xcarchive && rm -rf ios/build/Fasting-Release",
    "ios:fas:create:file": "xcodebuild -workspace ios/App.xcworkspace -scheme Fasting -sdk iphoneos -configuration Release archive -archivePath ios/build/Fasting-Release.xcarchive",
    "ios:fas:build:file": "xcodebuild -exportArchive -archivePath ios/build/Fasting-Release.xcarchive -exportOptionsPlist ios/App/Fasting.release.plist -exportPath ios/build/Fasting-Release",
    "ios:fas:build": "yarn ios:fas:clean:build && yarn ios:fas:create:file && yarn ios:fas:build:file",
    "ios:fas:upload:build": "xcrun altool --upload-app -f ios/build/Fasting-Release/Fasting.ipa -u viniciusnw@hotmail.com -p vwgj-mojs-ndwj-kgkg",
    "ios:fas:build:publish": "yarn ios:fas:build && yarn ios:fas:upload:build",
    "==": "==BUILD-PUBLISH/ios",
    "ios:fas:up:patch:publish": "yarn ios:fas:up:version:patch && yarn pause && yarn ios:fas:build:publish",
    "ios:fas:up:minor:publish": "yarn ios:fas:up:version:minor && yarn pause && yarn ios:fas:build:publish",
    "ios:fas:up:major:publish": "yarn ios:fas:up:version:major && yarn pause && yarn ios:fas:build:publish",
    "ios:fas:up:bundler:publish": "yarn ios:fas:up:bundler && yarn pause && yarn ios:fas:build:publish"
  },
  "dependencies": {
    "@apollo/client": "^3.3.14",
    "@ptomasroos/react-native-multi-slider": "2.2.2",
    "@react-native-community/async-storage": "1.11.0",
    "@react-native-community/blur": "^3.6.0",
    "@react-native-community/geolocation": "2.0.2",
    "@react-native-community/masked-view": "0.1.10",
    "@react-navigation/drawer": "5.8.1",
    "@react-navigation/material-top-tabs": "5.2.9",
    "@react-navigation/native": "5.5.0",
    "@react-navigation/stack": "5.4.1",
    "es6-symbol": "3.1.1",
    "graphql": "^15.5.0",
    "react": "17.0.1",
    "react-native": "0.62.2",
    "react-native-circular-progress": "^1.3.7",
    "react-native-config": "github:luggit/react-native-config#1eb6ac0",
    "react-native-fast-image": "8.3.4",
    "react-native-gesture-handler": "1.9.0",
    "react-native-multi-slider": "0.3.6",
    "react-native-progress": "^4.1.2",
    "react-native-reanimated": "1.13.2",
    "react-native-safe-area-context": "3.1.9",
    "react-native-screens": "2.17.1",
    "react-native-slider": "0.11.0",
    "react-native-snap-carousel": "3.9.1",
    "react-native-splash-screen": "^3.2.0",
    "react-native-svg": "12.1.0",
    "react-native-tab-view": "2.15.2",
    "react-native-vector-icons": "8.0.0",
    "react-redux": "7.2.2",
    "reconnecting-websocket": "^4.4.0",
    "redux": "4.0.5",
    "redux-devtools-extension": "2.13.8",
    "redux-persist": "6.0.0",
    "redux-promise-middleware": "6.1.2",
    "reflect-metadata": "0.1.13",
    "styled-components": "5.2.1",
    "switch-button-react-native": "^1.0.3",
    "typedi": "0.10.0"
  },
  "devDependencies": {
    "@babel/core": "7.10.1",
    "@babel/plugin-proposal-decorators": "^7.13.15",
    "@babel/runtime": "7.10.1",
    "@react-native-community/eslint-config": "1.1.0",
    "@storybook/addon-actions": "3.4.11",
    "@storybook/addon-links": "3.4.11",
    "@storybook/addons": "3.4.11",
    "@storybook/react-native": "5.3.19",
    "@types/react": "16.9.35",
    "@types/react-dom": "16.0.7",
    "@types/react-native": "0.62.12",
    "@types/storybook__addon-links": "3.3.2",
    "@types/storybook__react": "3.0.9",
    "@types/storybook__react-native": "3.0.2",
    "@types/styled-components": "5.1.0",
    "apollo": "2.32.5",
    "babel-core": "6.26.3",
    "babel-plugin-import-graphql": "^2.8.1",
    "babel-plugin-module-resolver": "4.0.0",
    "babel-runtime": "6.26.0",
    "concurrently": "4.0.1",
    "edit-json-file": "^1.5.0",
    "jetifier": "1.6.5",
    "metro-react-native-babel-preset": "0.59.0",
    "prop-types": "15.6.2",
    "react-dom": "16.3.1",
    "react-native-scripts": "1.14.0",
    "react-native-storybook-loader": "1.8.1",
    "react-native-svg-transformer": "^0.14.3",
    "react-native-version": "^4.0.0",
    "rimraf": "2.6.2",
    "simple-plist": "^1.1.1",
    "ts-node": "^9.1.1",
    "typescript": "3.9.3"
  },
  "jest": {
    "preset": "react-native"
  }
}
